{
  "name": "lib-galimatias",
  "description": "Galimatias library",
  "license": "Apache 2.0",
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --color",
    "build:server": "webpack --config webpack.server.config.js --color",
    "build": "run-p -c build:*",
    "dist:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --color",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.14.4",
    "@babel/register": "7.13.16",
    "babel-loader": "8.2.2",
    "browserslist-config-enonic": "1.0.6",
    "cross-env": "7.0.3",
    "file-loader": "6.2.0",
    "glob": "7.1.7",
    "npm-run-all": "4.1.5",
    "ramda": "0.27.1",
    "terser-webpack-plugin": "5.1.3",
    "ts-loader": "9.2.2",
    "typescript": "4.3.2",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 14.16.0",
    "yarn": ">= 1.22.4"
  },
  "private": true
}
